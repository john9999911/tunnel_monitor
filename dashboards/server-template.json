{
  "title": "Tunnel Server 监控面板",
  "tags": ["tunnel-server", "prometheus"],
  "timezone": "browser",
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "Percentile",
        "type": "custom",
        "query": "50,75,90,95,99",
        "current": {
          "text": "95",
          "value": "95"
        },
        "options": [
          {"text": "50", "value": "50"},
          {"text": "75", "value": "75"},
          {"text": "90", "value": "90"},
          {"text": "95", "value": "95"},
          {"text": "99", "value": "99"}
        ],
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 0
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "HTTP请求率",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(tunnel_server_http_requests_total[5m])",
          "legendFormat": "{{method}} {{endpoint}}",
          "datasource": {
            "type": "prometheus",
            "uid": "ef32in03bdb0gb"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "tooltip": false,
              "vis": false,
              "legend": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "color": {
            "mode": "palette-classic"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          },
          "unit": "reqps",
          "decimals": 2
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "legend": {
        "displayMode": "list",
        "placement": "right",
        "showLegend": true,
        "values": ["last"],
        "calcs": ["lastNotNull"]
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right",
          "showLegend": true,
          "values": ["last"],
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 2,
      "title": "HTTP响应时间",
      "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile($Percentile / 100.0, sum(rate(tunnel_server_http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "{{endpoint}} (p$Percentile)",
            "datasource": {
              "type": "prometheus",
              "uid": "ef32in03bdb0gb"
            }
          }
        ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "tooltip": false,
              "vis": false,
              "legend": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "color": {
            "mode": "palette-classic"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          },
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        }
      }
    },
    {
