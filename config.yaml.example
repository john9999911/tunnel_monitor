# Tunnel Monitor 配置文件示例

# Prometheus 配置
prometheus:
  url: "http://localhost:9090"
  port: 9090
  data_dir: "./prometheus_data"
  config_file: "./config/monitoring/prometheus.yml"

# Grafana 配置
grafana:
  url: "http://localhost:3000"
  port: 3000
  username: "admin"
  password: "admin"
  # api_key: ""  # 可选，使用 API Key 替代用户名密码

# 服务端配置
server:
  metrics_url: "http://localhost:8001/metrics"
  port: 8001

# 客户端配置列表（可以从 Prometheus 自动发现，也可以手动配置）
# 如果为空，将从 Prometheus 查询自动发现客户端实例
clients: []

# 面板模板路径（相对于 tunnel_monitor 目录）
dashboards:
  server_template: "./dashboards/server-template.json"
  client_template: "./dashboards/client-template.json"
  database_template: "./dashboards/database-template.json"
  unified_uid: "pop-clients-unified"
  server_uid: "tunnel-server"
  database_uid: "tunnel-database"

